[alias]
  lg = log --pretty=format:'%h - %an [%ar] %s'
  l = log --oneline --decorate --graph --all
  co = checkout
  cob = checkout -b
  ci = commit
  cim = commit -m
  st = status
  br = branch

[push]
  default = current

[user]
  name = charlieroth
  email = charlieroth4@gmail.com

[github]
  user = charlieroth

[credential]
  username = charlieroth

[apply]
  whitespace = nowarn

[help]
  autocorrect = 1

[core]
  excludesfile = ~/.gitignore_global
  editor = vim

[log]
  date = relative

[init]
  defaultBranch = main

[filter "lfs"]
  required = true
  clean = git-lfs clean -- %f
  smudge = git-lfs smudge -- %f
  process = git-lfs filter-process
